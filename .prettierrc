{
	"tabWidth": 4,
	"useTabs": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte",
				"tabWidth": 2,
				"useTabs": false
			}
		},
		{
			"files": ["*.ts", "*.js"],
			"options": {
				"tabWidth": 4,
				"useTabs": true
			}
		}
	],
	"tailwindStylesheet": "./src/routes/layout.css"
}
